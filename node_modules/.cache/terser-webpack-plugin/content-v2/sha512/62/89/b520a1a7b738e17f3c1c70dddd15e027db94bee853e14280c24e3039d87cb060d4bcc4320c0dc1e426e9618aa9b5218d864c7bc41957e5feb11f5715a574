={loading}\\n                    className={'category-modal__actions-cancel'}\\n                >\\n                    Cancel\\n                </Button>\\n                <Button\\n                    onClick={this.onActionAndAddClick}\\n                    variant='contained'\\n                    type={'submit'}\\n                    color={'primary'}\\n                    disabled={loading || invalid}\\n                    className={'category-modal__actions-save'}\\n                >\\n                    {mode === 'add' ? 'Save' : 'Update'} and add another\\n                </Button>\\n                <Button\\n                    onClick={this.onActionAndCLoseCLick}\\n                    variant='contained'\\n                    color={'primary'}\\n                    type={'submit'}\\n                    disabled={loading || invalid}\\n                    className={'category-modal__actions-save'}\\n                >\\n                    {mode === 'add' ? 'Save' : 'Update'} and close\\n                </Button>\\n            </div>\\n        );\\n    }\\n\\n    renderDialogTitle() {\\n        const { mode } = this.props;\\n        const word = mode === 'add' ? 'Add' : 'Edit';\\n\\n        return `${word} Dish`;\\n    }\\n\\n    render() {\\n        const {\\n            loading,\\n            open,\\n            change,\\n            setSameExtra,\\n            setSameSize,\\n            category_id,\\n            sizes,\\n            picture\\n        } = this.props;\\n        const { activeTab } = this.state;\\n\\n        return (\\n            <Dialog\\n                open={open}\\n                onEntered={this.onEntered}\\n                fullWidth={true}\\n                maxWidth={'md'}\\n                className={'dish-modal'}\\n            >\\n                <form name={'category'} className={'dish-modal-form'}>\\n                    {loading && <AppPreloaderSmall />}\\n                    <DialogTitle>{this.renderDialogTitle()}</DialogTitle>\