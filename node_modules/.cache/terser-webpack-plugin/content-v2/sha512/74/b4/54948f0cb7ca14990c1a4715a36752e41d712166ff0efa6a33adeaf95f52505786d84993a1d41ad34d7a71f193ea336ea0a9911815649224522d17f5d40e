();\\n        event.stopPropagation();\\n        this.requestAction().promise.then(this.onCloseClick);\\n    }\\n\\n    @autobind\\n    onActionAndAddClick(event) {\\n        event.preventDefault();\\n        event.stopPropagation();\\n\\n        const { setMode, reset, setAddState } = this.props;\\n        this.requestAction().promise.then(() => {\\n            setMode('add');\\n            reset();\\n            setAddState();\\n            this.setState({ activeTab: 0 });\\n        });\\n    }\\n\\n    renderDialogActions() {\\n        const { loading, mode } = this.props;\\n        const values = HelperService.pick(this.props, [\\n            'name',\\n            'description',\\n            'price',\\n            'extra',\\n            'sizes'\\n        ]);\\n\\n        const invalid = !validateForButtons(values);\\n\\n        return (\\n            <div className={'category-modal__actions'}>\\n                <Button\\n                    onClick={this.onCloseClick}\\n                    variant='contained'\\n                    disabled={loading}\\n                    className={'category-modal__actions-cancel'}\\n                >\\n                    Cancel\\n                </Button>\\n                <Button\\n                    onClick={this.onActionAndAddClick}\\n                    variant='contained'\\n                    type={'submit'}\\n                    color={'primary'}\\n                    disabled={loading || invalid}\\n                    className={'category-modal__actions-save'}\\n                >\\n                    {mode === 'add' ? 'Save' : 'Update'} and add another\\n                </Button>\\n                <Button\\n                    onClick={this.onActionAndCLoseCLick}\\n                    variant='contained'\\n                    color={'primary'}\\n                    type={'submit'}\\n                    disabled={loading || invalid}\\n                    className={'category-modal__actions-save'}\\n                >\