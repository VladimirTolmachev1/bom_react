\\n        }\\n\\n        case INCREASE_CHECKOUT_ITEM_AMOUNT: {\\n            return {\\n                ...state,\\n                [ payload.uuid ]: {\\n                    ...state[ payload.uuid ],\\n                    amount: state[ payload.uuid ].amount + payload.amountToAdd\\n                }\\n            };\\n        }\\n\\n        case DECREASE_CHECKOUT_ITEM_AMOUNT: {\\n            return {\\n                ...state,\\n                [ payload.uuid ]: {\\n                    ...state[ payload.uuid ],\\n                    amount: state[ payload.uuid ].amount - payload.amountToSubstruct\\n                }\\n            };\\n        }\\n\\n        case SET_CHECKOUT_ITEM_SIZE: {\\n            return {\\n                ...state,\\n                [ payload.uuid ]: {\\n                    ...state[ payload.uuid ],\\n                    selectedSize: payload.sizeId\\n                }\\n            };\\n        }\\n\\n        case ADD_NEW_EXTRA_TO_CHECKOUT_ITEM: {\\n            const { uuid, extraId } = payload;\\n            return {\\n                ...state,\\n                [ uuid ]: setItemExtraAmount(state[ uuid ], extraId, 1)\\n            };\\n        }\\n\\n        case REMOVE_EXTRA_FROM_CHECKOUT_ITEM: {\\n            const { uuid, extraId } = payload;\\n            return {\\n                ...state,\\n                [ uuid ]: setItemExtraAmount(state[ uuid ], extraId, null)\\n            };\\n        }\\n\\n        case INCREASE_CHECKOUT_ITEM_EXTRA_AMOUNT: {\\n            const { uuid, extraId, amountToAdd } = payload;\\n            const amount = state[ uuid ].extrasAmountById[ extraId ] + amountToAdd;\\n\\n            return {\\n                ...state,\\n                [ uuid ]: setItemExtraAmount(state[ uuid ], extraId, amount)\\n            };\\n        }\\n\\n        case DECREASE_CHECKOUT_ITEM_EXTRA_AMOUNT: {\\n            const { uuid, extraId, amountToSubstruct } = payload;\\n            const amount = state[ uuid ].extrasAmountByI