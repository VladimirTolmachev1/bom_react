es';\\nimport { DishOrderTotal } from '../../../Menu/components';\\n\\nimport './ReviewOrderTab.scss';\\n\\nexport class ReviewOrderTab extends PureComponent {\\n    mapOrderItems() {\\n        const { order } = this.props;\\n\\n        return order.map((dish, index) => {\\n            return (\\n                <ReviewOrderItem\\n                    key={index}\\n                    {...{ dish }}\\n                    {...HelperService.pick(this.props, [\\n                        'removeDish',\\n                        'setDishExtra',\\n                        'removeDishExtra',\\n                        'setDishSize'\\n                    ])}\\n                />\\n            );\\n        });\\n    }\\n\\n    render() {\\n        const { restaurant_url, handleNextStep, totalPrice, canNextStep } = this.props;\\n\\n        return (\\n            <div className={'review-order-tab'}>\\n                <div className='review-order-tab__content'>\\n                    <div className='review-order-tab__items'>\\n                        {this.mapOrderItems()}\\n                    </div>\\n\\n                    <DishOrderTotal\\n                        showCheckoutBtn={false}\\n                        className={'review-order-tab__total'}\\n                        {...HelperService.pick(totalPrice, [\\n                            'subtotal',\\n                            'deliveryFee',\\n                            'salesTax',\\n                            'total'\\n                        ])}\\n                    />\\n\\n                    <div className='review-order-tab__step-controls'>\\n                        <Link to={`/r/${restaurant_url}`} className={'app-invisible-link'}>\\n                            <Button\\n                                variant={'contained'}\\n                                color={'secondary'}\\n                                className={'review-order-tab__back-menu'}\\n                            >\\n                                B