  id: null\\n});\\n\\nexport function reducer(state = getInitialState(), action) {\\n    switch (action.type) {\\n        case UPDATE_DISH_REQUEST:\\n        case GET_DISH_BY_ID_REQUEST:\\n        case CREATE_DISH_REQUEST: {\\n            return {\\n                ...state,\\n                loading: true\\n            }\\n        }\\n\\n        case CLEAR_MODAL_DATA: {\\n            return getInitialState();\\n        }\\n\\n        case GET_DISH_BY_ID_SUCCESS: {\\n            return {\\n                ...state,\\n                initialValues: action.response,\\n                loading: false\\n            }\\n        }\\n\\n        case UPDATE_DISH_SUCCESS:\\n        case CREATE_DISH_SUCCESS:\\n        case CREATE_DISH_ERROR: {\\n            return {\\n                ...state,\\n                loading: false\\n            }\\n        }\\n\\n        case SET_MODAL_ADD_INITIAL_STATE: {\\n            return {\\n                ...getInitialState(),\\n                open: true\\n            }\\n        }\\n\\n        case SET_DISH_MODAL_MODE: {\\n            return {\\n                ...state,\\n                mode: action.mode\\n            }\\n        }\\n\\n        case GET_DISH_BY_ID_ERROR:\\n        case UPDATE_DISH_ERROR: {\\n            return {\\n                ...state,\\n                loading: false,\\n                error: action.error\\n            }\\n        }\\n\\n        case TOGGLE_DISH_MODAL: {\\n            const {id, open} = action;\\n            return {\\n                ...state,\\n                open,\\n                id\\n            }\\n        }\\n\\n        default: {\\n            return state\\n        }\\n    }\\n}\\n\\nexport function dishReducerPlugin(state, action) {\\n    switch(action.type) {\\n        case SET_SAME_EXTRA:\\n            const { extra } = action.dish;\\n            return {\\n                ...state,\\n                values: {\\n                    ...state.values,\\n                    extra,\\n     