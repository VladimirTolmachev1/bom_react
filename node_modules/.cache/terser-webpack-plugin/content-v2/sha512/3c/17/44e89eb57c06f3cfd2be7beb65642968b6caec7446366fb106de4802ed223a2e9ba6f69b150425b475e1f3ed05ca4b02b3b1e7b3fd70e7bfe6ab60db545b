      GET_ORDERS_TOTAL_INFO_REQUEST,\\n        GET_ORDERS_TOTAL_INFO_SUCCESS,\\n        GET_ORDERS_TOTAL_INFO_ERROR,\\n    ]\\n} = ReduxCrudService.getActionCrudTypes(orderTotalInfoEndpoint);\\n\\n\\n\\n\",\"import { normalize } from 'normalizr';\\nimport { ordersSchema } from './orders.schema';\\n\\nimport { ReduxCrudService } from '/services';\\nimport {\\n    resource,\\n    orderTotalInfoEndpoint,\\n    SET_UPDATED_ORDER_IN_STORE_DIRECTLY,\\n} from './orders.types';\\n\\nexport const getOrders = ReduxCrudService.resourceAction({\\n    resource,\\n    method: 'GET',\\n    responseConverter: (res) => {\\n        const { entities, result : allIds } = normalize(res.data, ordersSchema);\\n\\n        return { \\n            ...res,\\n            data: {\\n                byId: entities.ordersById,\\n                allIds,\\n            },\\n        }\\n    }\\n});\\n\\nexport const getOrderById = ReduxCrudService.resourceAction({\\n    resource,\\n    method: 'GET',\\n    byId: true\\n});\\n\\nexport const updateOrder = ReduxCrudService.resourceAction({\\n    resource,\\n    method: 'PATCH'\\n});\\n\\nexport const getOrdersTotalInfoByRestaurantId = ReduxCrudService.resourceAction({\\n    resource: orderTotalInfoEndpoint,\\n    method: 'GET',\\n    byId: true,\\n})\\n\\nexport const setUpdatedOrderInStore = (order) => ({\\n    type: SET_UPDATED_ORDER_IN_STORE_DIRECTLY,\\n    payload: { ...order }\\n})\\n\\n\\n\",\"import {\\n    SET_UPDATED_ORDER_IN_STORE_DIRECTLY,\\n\\n    GET_ORDERS_REQUEST,\\n    GET_ORDERS_SUCCESS,\\n    GET_ORDERS_ERROR,\\n    UPDATE_ORDER_SUCCESS,\\n\\n    GET_ORDERS_TOTAL_INFO_SUCCESS,\\n    \\n} from './orders.types';\\n\\nconst initialState = {\\n    byId: {},\\n    total: {\\n        ordersCompleted: 0,\\n        revenue: 0,\\n    },\\n    allIds: [],\\n    managed: null,\\n    isOrdersListLoading: false,\\n}\\n\\nexport function ordersReducer (state = initialState, action) {\\n    switch( action.type ) {\\n        case GET_ORDERS_SUCCESS: {\\n            return { \\n                ...state,\\n                ...action.response.data,\\n                isOrdersListLoading: false,\\n            }\\n        }\\n\\n        case UPDATE_ORDER_SUCCESS: {\\n            const { id } = action.response;\\n            return {\\n                ...state,\\n                byId: {\\n                    ...state.byId,\\n                    [ id ]: { ...action.response },\\n                }\\n            }\\n        }\\n\\n        case SET_UPDATED_ORDER_IN_STORE_DIRECTLY: {\\n            const { id } = action.payload;\\n\\n            return {\\n                ...state,\\n                byId: {\\n                    ...state.byId,\\n                    [ id ]: { \\n                        ...state.byId[ id ],\\n                        ...action.payload\\n                    }\\n                }\\n            }\\n        }\\n\\n        case GET_ORDERS_TOTAL_INFO_SUCCESS: {\\n            return {\\n                ...state,\\n                total: { \\n                    ordersCompleted: action.response.total_orders,\\n                    revenue: action.response.total_revenue\\n                }\\n            }\\n        }\\n\\n        case GET_ORDERS_REQUEST: {\\n            return {\\n                ...state,\\n                isOrdersListLoading: true,\\n            }\\n        }\\n\\n        case GET_ORDERS_ERROR: {\\n            return {\\n                ...state,\\n                isOrdersListLoading: false,\\n            }\\n        }\\n\\n        default: return state;\\n    }\\n}\\n\",\"import { createSelector } from 'reselect';\\nimport { orderDeliveryMethods, orderStatuses } from './../../constants';\\n\\nconst { DELIVERY, PICKUP } = orderDeliveryMethods;\\nconst { NEW, IN_PROCESS, IN_DELIVERY, READY, REFUND, COMPLETE, PENDING } = orderStatuses;\\n\\n\\nexport const ordersByIdsSelector = ({ orders }) => orders.byId;\\nexport const allOrdersIdsSelector = ({ orders }) => orders.allIds;\\nexport const