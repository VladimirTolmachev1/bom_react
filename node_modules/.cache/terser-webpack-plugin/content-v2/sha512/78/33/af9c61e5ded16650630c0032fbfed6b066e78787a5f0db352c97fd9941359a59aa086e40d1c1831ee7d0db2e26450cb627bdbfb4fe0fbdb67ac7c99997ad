Search,\\n            customTableHeader\\n        } = this.getTableOptions();\\n\\n        const {\\n            limit,\\n            search,\\n            page,\\n            data,\\n            total,\\n            loading\\n        } = this.props;\\n\\n        return (\\n            <div className={ `voc-table__container ${loading && 'voc-table__container-loading'}` }>\\n                <AppTableHeader\\n                    { ...{\\n                        haveSearch,\\n                        search,\\n                        onSearchType\\n                    } }>\\n                    { customTableHeader }\\n                </AppTableHeader>\\n                <div className={ `voc-table__wrapper ${loading && 'voc-table__wrapper-loading'}` }>\\n                    { loading && <AppPreloaderSmall/> }\\n                    <Table className={ 'voc-table' }>\\n                        <TableHead className={ 'voc-table__header' }>\\n                            <TableRow style={ rowStyle }>\\n                                { this.mapTableHead() }\\n                            </TableRow>\\n                        </TableHead>\\n                        <TableBody className={ 'voc-table__body' }>\\n                            { data.length ? this.mapTableBodyRows() : this.getEmptyTable() }\\n                        </TableBody>\\n                        <AppTableFooter\\n                            { ...{\\n                                total,\\n                                page,\\n                                limit,\\n                                onPageChange,\\n                                onLimitChange\\n                            } }\\n                        />\\n                    </Table>\\n                </div>\\n            </div>\\n        );\\n    }\\n}\\n\\nAppTable.propTypes = {\\n    data: PropTypes.array.isRequired,\\n    total: PropTypes.number.isRequired,\\n    loading: PropTypes.bool.isRequired,\\n\\n    dataSettings: PropTypes.arrayO