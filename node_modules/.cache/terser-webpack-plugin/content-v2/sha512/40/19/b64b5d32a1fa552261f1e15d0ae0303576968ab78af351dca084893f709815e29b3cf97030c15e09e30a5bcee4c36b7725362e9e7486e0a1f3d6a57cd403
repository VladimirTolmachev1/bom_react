Size ? +selectedSize.price : dish.price;\\n    let totalPrice = price;\\n    const amount = dish.amount || 1;\\n\\n    if(selectedExtras.length){\\n        totalPrice += getSelectedExtrasPrice(selectedExtras);\\n    }\\n\\n    totalPrice = totalPrice * amount;\\n\\n    return {\\n        ...dish,\\n        price,\\n        selectedExtras,\\n        selectedSize,\\n        totalPrice,\\n        amount        \\n    };\\n}\\n\\nfunction getDishSelectedSize({ selectedSize, sizes = []}) {\\n    if(!selectedSize || !sizes.length) {\\n        return null;\\n    }\\n\\n    return sizes.find( ({id}) => +id === +selectedSize);\\n}\\n\\n    \\nexport function getSelectedExtrasPrice(selectedExtras) {\\n    return selectedExtras.reduce( \\n        (sum, { price, amount }) => sum += price * amount, \\n        0 \\n    )\\n}\\n\\n\\nexport function getSelectedExtrasByAllAndAmountByIdsObj(extras = [], extrasAmountById = {}) {\\n    const selectedExtrasIds = Object.keys(extrasAmountById)\\n        .filter( id => !!extrasAmountById[ id ] );\\n\\n    const selectedExtras = selectedExtrasIds.map( extraId => ({\\n        ...extras.find(({ id }) => +id === +extraId ),\\n        amount: extrasAmountById[ extraId ]\\n    }))\\n    \\n    return selectedExtras;\\n}\\n\\n\\nexport const getUnicOrderItemDescriptor = ({ id, selectedExtras, selectedSize }) => {\\n    let dishDescriptor = `dishId#${ id }`;\\n\\n    let sizeId;\\n\\n    if( selectedSize ) {\\n        sizeId = sizeId = typeof selectedSize === 'object' ? selectedSize.id : selectedSize;\\n\\n        dishDescriptor += `#sizeId${ sizeId }#`;\\n    }\\n\\n    const extrasDescriptor = selectedExtras.reduce(\\n        (descriptor, { id, amount }) => `${ descriptor }${ id }:${ amount }`,\\n        ''\\n    )\\n\\n    dishDescriptor += extrasDescriptor;\\n\\n    return dishDescriptor;\\n}\\n\\n\\nexport const prepeareOrderFormPayment = (orderItems) => {\\n    const mergedItemsByDescriptors = orderItems.reduce(\\n        (itemsByDescriptor