({ uuid }){\\n        const { actions } = this.props;\\n        actions.removeItemFromCheckout(uuid);\\n    }\\n\\n    @autobind\\n    handleIncreaseItemAmount({ uuid }){\\n        const { actions } = this.props;\\n        actions.increaseCheckoutItemAmountBy(uuid, 1);\\n    }\\n\\n    @autobind\\n    handleDecreaseItemAmount({ uuid }){\\n        const { actions } = this.props;\\n        actions.decreaseCheckoutItemAmountBy(uuid, 1);\\n    }\\n\\n    @autobind\\n    onPaymentDetailsSubmit(paymentDetails) {\\n        const { actions } = this.props;\\n        actions.createOrderFromCheckout({ paymentDetails });\\n        // this.handleNextStep();\\n    }\\n\\n    getRestaurantLink() {\\n        // REPLACE: move to selector\\n\\n        const { \\n            history: { location: { pathname } }\\n        } = this.props;\\n\\n        const restaurant_url = pathname.split('/')[2];\\n\\n        return `/r/${restaurant_url}/Category`;\\n    }\\n\\n    getStepContent(step) {\\n        const {\\n            history: {\\n                location: { pathname }\\n            },\\n            actions,\\n            totalPrice,\\n            order,\\n        } = this.props;\\n\\n        const restaurant_url = pathname.split('/')[2];\\n\\n        switch (step) {\\n            case 0: {\\n                return (\\n                    <ReviewOrderTab\\n                        totalPrice={totalPrice}\\n                        canNextStep={ !!order.length }\\n                        order={ order }\\n                        restaurant_url={ restaurant_url }\\n                        handleNextStep={ actions.handleNextStep }\\n                        restaurantLink={ this.getRestaurantLink() }\\n                        handleRemoveDishExtra={ this.handleRemoveDishExtra }\\n                        handleAddDishExtra={ this.handleAddDishExtra }\\n                        handleSubstructDishExtra={ this.handleSubstructDishExtra }\\n                        handleSelectDishSize={ this.handl