s.map( extraId => ({\\n        ...extras.find(({ id }) => +id === +extraId ),\\n        amount: extrasAmountById[ extraId ]\\n    }))\\n    \\n    return selectedExtras;\\n}\\n\\n\\nexport const getUnicOrderItemDescriptor = ({ id, selectedExtras, selectedSize }) => {\\n    let dishDescriptor = `dishId#${ id }`;\\n\\n    let sizeId;\\n\\n    if( selectedSize ) {\\n        sizeId = sizeId = typeof selectedSize === 'object' ? selectedSize.id : selectedSize;\\n\\n        dishDescriptor += `#sizeId${ sizeId }#`;\\n    }\\n\\n    const extrasDescriptor = selectedExtras.reduce(\\n        (descriptor, { id, amount }) => `${ descriptor }${ id }:${ amount }`,\\n        ''\\n    )\\n\\n    dishDescriptor += extrasDescriptor;\\n\\n    return dishDescriptor;\\n}\\n\\n\\nexport const prepeareOrderFormPayment = (orderItems) => {\\n    const mergedItemsByDescriptors = orderItems.reduce(\\n        (itemsByDescriptors, item) => {\\n            const newItem = {\\n                id: item.id,\\n                amount: item.amount || 1,\\n                selectedSize: (item.selectedSize || {}).id, // suppose that selectedSize is an object\\n                selectedExtras: item.selectedExtras.map(({id, amount }) => ({ id, amount }))\\n            }\\n\\n            const descriptor = getUnicOrderItemDescriptor(newItem);\\n\\n            if(itemsByDescriptors[ descriptor ]) {\\n                itemsByDescriptors[ descriptor ].amount += newItem.amount;\\n            } else {\\n                itemsByDescriptors[ descriptor ] = newItem;\\n            }\\n\\n            return itemsByDescriptors;\\n         }, \\n         {} \\n    );\\n\\n    return Object.values(mergedItemsByDescriptors);\\n}\\n\\nexport const generateCkeckoutStorageKeyByRestaurantId = id => `CHECKOUT_FOR_RESTAURANT_WITH_ID_${ id }`;\\n\",\"\\nexport const OPEN_DISH_ORDER_MODAL = 'OPEN_DISH_ORDER_MODAL';\\nexport const CLOSE_DISH_ORDER_MODAL = 'CLOSE_DISH_ORDER_MODAL';\\n\",\"export const FORM_FIELDS_TO_SELECT = [\\n    'id',\\n    'name',\\n    'price',\\n    'sizes',\\n    'extra',\\n    'amount',\\n    'picture',\\n    'selectedSize',\\n    'selectedExtras',\\n    'extrasAmountById',\\n];\\n\\nexport const ORDER_PRODUCT = 'ORDER PRODUCT FORM';\\n\",\"import { createSelector } from 'reselect';\\nimport { formValueSelector } from 'redux-form';\\nimport {\\n    ORDER_PRODUCT,\\n    FORM_FIELDS_TO_SELECT\\n} from './constants';\\n\\nimport { allDishesByIdsSelector } from './../../dishes';\\nimport { dishPriceAndSelectedExtrasDecorator } from './../../checkout/checkout.helpers';\\n\\nexport const dishOrderFormFieldsSelector = formValueSelector(ORDER_PRODUCT);\\n\\n\\nexport const dishOrderFormValuesSelector = (state) => {\\n    let values = dishOrderFormFieldsSelector(state, ...FORM_FIELDS_TO_SELECT);\\n\\n    return dishPriceAndSelectedExtrasDecorator(values);;\\n}\\n\\nexport const activeDishIdForDishOrderModalSelector = ({ modals: { dishOrder }}) => {\\n    return dishOrder.dishId\\n}\\n\\nexport const isDishOrderModalOpenSelector = ({ modals: { dishOrder }}) => !!dishOrder.open;\\n\\n\\nconst emptyObject = {}\\nexport const activeDishForDishOrderModalSelector = createSelector(\\n    [ allDishesByIdsSelector, activeDishIdForDishOrderModalSelector ],\\n    (dishesById, id) => {\\n        return dishesById[ id ] || emptyObject\\n    }\\n)\\n\\nexport const dishOrderInitialValuesSelector = createSelector(\\n    [ activeDishForDishOrderModalSelector ],\\n    (dish) => {\\n       \\n        const res = {\\n            ...dish,\\n            selectedSize: (dish.sizes || []).length ? dish.sizes[0].id : null,\\n            extrasAmountById: {},\\n        }\\n\\n        return res;\\n    }\\n)\\n\\n\\n\\n\\n\\n\\n\\n\\n\\n\",\"\\n\\nimport { \\n    OPEN_DISH_ORDER_MODAL,\\n    CLOSE_DISH_ORDER_MODAL,\\n} from './types';\\n\\nexport const openDishOrderModal = ({dishId, props = {}}) => ({\\n    type: OPEN_DISH_ORDER_MODAL,\\n    payload: { dishId, ...props }\\n})\\n\\nexport const closeDishOr