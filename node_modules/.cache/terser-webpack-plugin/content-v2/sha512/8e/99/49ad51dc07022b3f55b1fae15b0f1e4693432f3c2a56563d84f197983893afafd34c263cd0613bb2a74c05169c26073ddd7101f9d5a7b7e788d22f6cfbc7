,\\n                [ uuid ]: setItemExtraAmount(state[ uuid ], extraId, null)\\n            };\\n        }\\n\\n        case INCREASE_CHECKOUT_ITEM_EXTRA_AMOUNT: {\\n            const { uuid, extraId, amountToAdd } = payload;\\n            const amount = state[ uuid ].extrasAmountById[ extraId ] + amountToAdd;\\n\\n            return {\\n                ...state,\\n                [ uuid ]: setItemExtraAmount(state[ uuid ], extraId, amount)\\n            };\\n        }\\n\\n        case DECREASE_CHECKOUT_ITEM_EXTRA_AMOUNT: {\\n            const { uuid, extraId, amountToSubstruct } = payload;\\n            const amount = state[ uuid ].extrasAmountById[ extraId ] - amountToSubstruct;\\n\\n            return {\\n                ...state,\\n                [ uuid ]: setItemExtraAmount(state[ uuid ], extraId, amount)\\n            };\\n        }\\n\\n        case CLEAR_CHECKOUT: {\\n            return initialState.itemsByUuids\\n        }\\n\\n        case RESTORE_CHECKOUT_FROM_LOCAL_STORAGE: {\\n            return payload.itemsByUuids;\\n        }\\n\\n        default: return state;\\n    }\\n}\\n\\nfunction uuidsByItemDecriptors( state = initialState.uuidsByItemDecriptors, { type, payload }) {\\n    switch(type) {\\n        case CREATE_NEW_CHECKOUT_ITEM: {\\n            return {\\n                ...state,\\n                [ payload.descriptor ]: payload.uuid\\n            };\\n        }\\n\\n        case REMOVE_ITEM_FROM_CHECKOUT: {\\n            return removeDescriptorByUuid(state, payload);\\n        }\\n\\n        case CLEAR_CHECKOUT: {\\n            return initialState.uuidsByItemDecriptors;\\n        }\\n\\n        case RESTORE_CHECKOUT_FROM_LOCAL_STORAGE: {\\n            return payload.uuidsByItemDecriptors;\\n        }\\n\\n        default: return state;\\n    }\\n}\\n\\nfunction charge(state = initialState.charge, action) {\\n    switch(action.type) {\\n        case CREATE_CHARGE_SUCCESS: {\\n            return {\\n                ...state, \\n              