 actions } = this.props;\\n        actions.removeExtraFromCheckoutItem(orderItem.uuid, dishExtra.id);\\n    }\\n\\n    @autobind\\n    handleAddDishExtra(extra, { uuid }){\\n        const { actions } = this.props;\\n        actions.increaseCheckoutItemExtrasAmountBy(uuid, extra.id, 1);\\n    }\\n\\n    @autobind\\n    handleSubstructDishExtra(extra, { uuid }){\\n        const { actions } = this.props;\\n        actions.decreaseCheckoutItemExtrasAmountBy(uuid, extra.id, 1);\\n    }\\n\\n    @autobind\\n    handleSelectDishSize({ selectedSize }, dish){\\n        const { actions } = this.props;\\n        actions.setCheckoutItemSize(dish.uuid, selectedSize);\\n    }\\n\\n    @autobind\\n    handleSelectDishExtra( { extraId }, orderItem) {\\n        const { actions } = this.props;\\n        actions.addNewExtraToCheckoutItem(orderItem.uuid, extraId);\\n    }\\n\\n\\n    @autobind\\n    handleRemoveOrderItem({ uuid }){\\n        const { actions } = this.props;\\n        actions.removeItemFromCheckout(uuid);\\n    }\\n\\n    @autobind\\n    handleIncreaseItemAmount({ uuid }){\\n        const { actions } = this.props;\\n        actions.increaseCheckoutItemAmountBy(uuid, 1);\\n    }\\n\\n    @autobind\\n    handleDecreaseItemAmount({ uuid }){\\n        const { actions } = this.props;\\n        actions.decreaseCheckoutItemAmountBy(uuid, 1);\\n    }\\n\\n    @autobind\\n    onPaymentDetailsSubmit(paymentDetails) {\\n        const { actions } = this.props;\\n        actions.createOrderFromCheckout({ paymentDetails });\\n        // this.handleNextStep();\\n    }\\n\\n    getRestaurantLink() {\\n\\n        const { \\n            match: { params }\\n        } = this.props;\\n\\n\\n        return `/r/${params.restaurant_url}`;\\n    }\\n\\n    @autobind\\n    onBackToMenuClick(){\\n        const { actions } = this.props;\\n        actions.backToRestaurantMenu(this.getRestaurantLink());\\n    }\\n\\n    getStepContent(step) {\\n        const {\\n            history: {\\n             